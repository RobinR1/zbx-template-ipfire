        <template>
            <template>Template App Pakfire by Zabbix Agent Active</template>
            <name>Template App Pakfire by Zabbix Agent Active</name>
            <description>Created by Alex Koch (ipfire@starkstromkonsument.de)</description>
            <groups>
                <group>
                    <name>Templates/Applications</name>
                </group>
            </groups>
            <applications>
                <application>
                    <name>Pakfire</name>
                </application>
                <application>
                    <name>Zabbix raw items</name>
                </application>
            </applications>
            <items>
                <item>
                    <name>Core-Update available</name>
                    <type>DEPENDENT</type>
                    <key>pakfire.core-update-available</key>
                    <delay>0</delay>
                    <history>7d</history>
                    <applications>
                        <application>
                            <name>Pakfire</name>
                        </application>
                    </applications>
                    <valuemap>
                        <name>Boolean Yes/No</name>
                    </valuemap>
                    <preprocessing>
                        <step>
                            <type>REGEX</type>
                            <params>Core-Update available: (.*)
\1</params>
                            <error_handler>DISCARD_VALUE</error_handler>
                        </step>
                        <step>
                            <type>JAVASCRIPT</type>
                            <params>// Convert Bool String to Bool Integer
switch(value) {
   case &quot;yes&quot;:
      return 1;
   case &quot;no&quot;:
      return 0;
   default:
      return 99;
}</params>
                        </step>
                        <step>
                            <type>IN_RANGE</type>
                            <params>0
1</params>
                            <error_handler>CUSTOM_ERROR</error_handler>
                            <error_handler_params>Unknown value received</error_handler_params>
                        </step>
                        <step>
                            <type>DISCARD_UNCHANGED_HEARTBEAT</type>
                            <params>1d</params>
                        </step>
                    </preprocessing>
                    <master_item>
                        <key>pakfire.status</key>
                    </master_item>
                    <triggers>
                        <trigger>
                            <expression>{last()}=1</expression>
                            <name>Pakfire: Core-Update available</name>
                            <url>https://{HOST.CONN}:444/cgi-bin/pakfire.cgi</url>
                            <priority>INFO</priority>
                            <description>A Core-Update for IPFire is available. Go to the IPFire webgui &gt; IPFire Pakfire to perform an update.</description>
                        </trigger>
                    </triggers>
                </item>
                <item>
                    <name>Core-Update-Level</name>
                    <type>DEPENDENT</type>
                    <key>pakfire.core-update-level</key>
                    <delay>0</delay>
                    <history>7d</history>
                    <applications>
                        <application>
                            <name>Pakfire</name>
                        </application>
                    </applications>
                    <preprocessing>
                        <step>
                            <type>REGEX</type>
                            <params>Core-Update-Level: ([0-9]*)
\1</params>
                            <error_handler>DISCARD_VALUE</error_handler>
                        </step>
                        <step>
                            <type>DISCARD_UNCHANGED_HEARTBEAT</type>
                            <params>1d</params>
                        </step>
                    </preprocessing>
                    <master_item>
                        <key>pakfire.status</key>
                    </master_item>
                    <triggers>
                        <trigger>
                            <expression>{diff()}=1</expression>
                            <name>Pakfire: Core-Update-Level has changed</name>
                            <priority>INFO</priority>
                            <dependencies>
                                <dependency>
                                    <name>Pakfire: Core-Version has changed</name>
                                    <expression>{Template App Pakfire by Zabbix Agent Active:pakfire.core-version.diff()}=1</expression>
                                </dependency>
                            </dependencies>
                        </trigger>
                    </triggers>
                </item>
                <item>
                    <name>Core-Version</name>
                    <type>DEPENDENT</type>
                    <key>pakfire.core-version</key>
                    <delay>0</delay>
                    <history>7d</history>
                    <value_type>FLOAT</value_type>
                    <applications>
                        <application>
                            <name>Pakfire</name>
                        </application>
                    </applications>
                    <preprocessing>
                        <step>
                            <type>REGEX</type>
                            <params>Core-Version: ([0-9.]*)
\1</params>
                            <error_handler>DISCARD_VALUE</error_handler>
                        </step>
                        <step>
                            <type>DISCARD_UNCHANGED_HEARTBEAT</type>
                            <params>1d</params>
                        </step>
                    </preprocessing>
                    <master_item>
                        <key>pakfire.status</key>
                    </master_item>
                    <triggers>
                        <trigger>
                            <expression>{diff()}=1</expression>
                            <name>Pakfire: Core-Version has changed</name>
                            <priority>INFO</priority>
                        </trigger>
                    </triggers>
                </item>
                <item>
                    <name>Package-Updates available</name>
                    <type>DEPENDENT</type>
                    <key>pakfire.package-updates-available</key>
                    <delay>0</delay>
                    <history>7d</history>
                    <applications>
                        <application>
                            <name>Pakfire</name>
                        </application>
                    </applications>
                    <preprocessing>
                        <step>
                            <type>REGEX</type>
                            <params>Package-Updates available: ([0-9]*)
\1</params>
                            <error_handler>DISCARD_VALUE</error_handler>
                        </step>
                    </preprocessing>
                    <master_item>
                        <key>pakfire.status</key>
                    </master_item>
                    <triggers>
                        <trigger>
                            <expression>{last()}&gt;0</expression>
                            <name>Pakfire: Package-Updates available</name>
                            <priority>INFO</priority>
                        </trigger>
                    </triggers>
                </item>
                <item>
                    <name>Reboot required</name>
                    <type>DEPENDENT</type>
                    <key>pakfire.reboot-required</key>
                    <delay>0</delay>
                    <history>7d</history>
                    <applications>
                        <application>
                            <name>Pakfire</name>
                        </application>
                    </applications>
                    <valuemap>
                        <name>Boolean Yes/No</name>
                    </valuemap>
                    <preprocessing>
                        <step>
                            <type>REGEX</type>
                            <params>Reboot required: (.*)
\1</params>
                            <error_handler>DISCARD_VALUE</error_handler>
                        </step>
                        <step>
                            <type>JAVASCRIPT</type>
                            <params>// Convert Bool String to Bool Integer
switch(value) {
   case &quot;yes&quot;:
      return 1;
   case &quot;no&quot;:
      return 0;
   default:
      return 99;
}</params>
                        </step>
                        <step>
                            <type>IN_RANGE</type>
                            <params>0
1</params>
                            <error_handler>CUSTOM_ERROR</error_handler>
                            <error_handler_params>Unknown value received</error_handler_params>
                        </step>
                        <step>
                            <type>DISCARD_UNCHANGED_HEARTBEAT</type>
                            <params>1d</params>
                        </step>
                    </preprocessing>
                    <master_item>
                        <key>pakfire.status</key>
                    </master_item>
                    <triggers>
                        <trigger>
                            <expression>{last()}=1</expression>
                            <name>Pakfire: Reboot required</name>
                            <url>https://{HOST.CONN}:444/cgi-bin/shutdown.cgi</url>
                            <priority>INFO</priority>
                            <description>An update requires a reboot of IPFire to complete. Please reboot the host as soon as possible. Go to the IPFire webgui &gt; System &gt; Shutdown to perform a reboot.</description>
                        </trigger>
                    </triggers>
                </item>
                <item>
                    <name>Pakfire Status</name>
                    <type>ZABBIX_ACTIVE</type>
                    <key>pakfire.status</key>
                    <delay>10m</delay>
                    <history>1d</history>
                    <trends>0</trends>
                    <value_type>TEXT</value_type>
                    <applications>
                        <application>
                            <name>Zabbix raw items</name>
                        </application>
                    </applications>
                </item>
                <item>
                    <name>Last update</name>
                    <type>ZABBIX_ACTIVE</type>
                    <key>vfs.file.time[&quot;/opt/pakfire/db/core/mine&quot;,modify]</key>
                    <delay>10m</delay>
                    <history>7d</history>
                    <units>unixtime</units>
                    <applications>
                        <application>
                            <name>Pakfire</name>
                        </application>
                    </applications>
                </item>
                <item>
                    <name>Last core-list update</name>
                    <type>ZABBIX_ACTIVE</type>
                    <key>vfs.file.time[&quot;/opt/pakfire/db/lists/core-list.db&quot;,modify]</key>
                    <delay>10m</delay>
                    <history>7d</history>
                    <units>unixtime</units>
                    <applications>
                        <application>
                            <name>Pakfire</name>
                        </application>
                    </applications>
                    <triggers>
                        <trigger>
                            <expression>{fuzzytime(176400)}=0</expression>
                            <recovery_mode>RECOVERY_EXPRESSION</recovery_mode>
                            <recovery_expression>{fuzzytime(3600)}=1</recovery_expression>
                            <name>Pakfire: Last core-list update &gt; 49h ago</name>
                            <priority>WARNING</priority>
                            <description>Auslösung bei Delta &gt; 49h / Recovery bei Delta &lt; 1h</description>
                            <dependencies>
