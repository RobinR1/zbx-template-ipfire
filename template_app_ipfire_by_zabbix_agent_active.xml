        <template>
            <template>Template Module IPFire Services by Zabbix Agent Active</template>
            <name>Template Module IPFire Services by Zabbix Agent Active</name>
            <description>Monitor status of IPFire Services&#13;
&#13;
Created by Robin Roevens (robin.roevens (at) disroot.org)</description>
            <groups>
                <group>
                    <name>Templates/Modules</name>
                </group>
            </groups>
            <applications>
                <application>
                    <name>IPFire Services</name>
                </application>
                <application>
                    <name>Zabbix raw items</name>
                </application>
            </applications>
            <items>
                <item>
                    <name>IPFire Services</name>
                    <type>ZABBIX_ACTIVE</type>
                    <key>ipfire.services</key>
                    <history>1d</history>
                    <trends>0</trends>
                    <value_type>TEXT</value_type>
                    <applications>
                        <application>
                            <name>Zabbix raw items</name>
                        </application>
                    </applications>
                </item>
            </items>
            <discovery_rules>
                <discovery_rule>
                    <name>IPFire Services discovery</name>
                    <type>DEPENDENT</type>
                    <key>ipfire.services.discovery</key>
                    <delay>0</delay>
                    <item_prototypes>
                        <item_prototype>
                            <name>{#SERVICE} Memory usage</name>
                            <type>DEPENDENT</type>
                            <key>ipfire.services.memory[{#SERVICENAME}]</key>
                            <delay>0</delay>
                            <history>7d</history>
                            <units>b</units>
                            <applications>
                                <application>
                                    <name>IPFire Services</name>
                                </application>
                            </applications>
                            <preprocessing>
                                <step>
                                    <type>JSONPATH</type>
                                    <params>$[?(@.servicename == &quot;{#SERVICENAME}&quot;)].memory.first()</params>
                                    <error_handler>DISCARD_VALUE</error_handler>
                                </step>
                            </preprocessing>
                            <master_item>
                                <key>ipfire.services</key>
                            </master_item>
                        </item_prototype>
                        <item_prototype>
                            <name>{#SERVICE} Start on boot</name>
                            <type>DEPENDENT</type>
                            <key>ipfire.services.onboot[{#SERVICENAME}]</key>
                            <delay>0</delay>
                            <history>7d</history>
                            <applications>
                                <application>
                                    <name>IPFire Services</name>
                                </application>
                            </applications>
                            <valuemap>
                                <name>Boolean Yes/No</name>
                            </valuemap>
                            <preprocessing>
                                <step>
                                    <type>JSONPATH</type>
                                    <params>$[?(@.servicename == &quot;{#SERVICENAME}&quot;)].onboot.first()</params>
                                    <error_handler>CUSTOM_VALUE</error_handler>
                                    <error_handler_params>1</error_handler_params>
                                </step>
                            </preprocessing>
                            <master_item>
                                <key>ipfire.services</key>
                            </master_item>
                        </item_prototype>
                        <item_prototype>
                            <name>{#SERVICE} Pid</name>
                            <type>DEPENDENT</type>
                            <key>ipfire.services.pid[{#SERVICENAME}]</key>
                            <delay>0</delay>
                            <history>7d</history>
                            <applications>
                                <application>
                                    <name>IPFire Services</name>
                                </application>
                            </applications>
                            <preprocessing>
                                <step>
                                    <type>JSONPATH</type>
                                    <params>$[?(@.servicename == &quot;{#SERVICENAME}&quot;)].pid.first()</params>
                                    <error_handler>DISCARD_VALUE</error_handler>
                                </step>
                            </preprocessing>
                            <master_item>
                                <key>ipfire.services</key>
                            </master_item>
                        </item_prototype>
                        <item_prototype>
                            <name>{#SERVICE} State</name>
                            <type>DEPENDENT</type>
                            <key>ipfire.services.state[{#SERVICENAME}]</key>
                            <delay>0</delay>
                            <history>7d</history>
                            <applications>
                                <application>
                                    <name>IPFire Services</name>
                                </application>
                            </applications>
                            <valuemap>
                                <name>Service state</name>
                            </valuemap>
                            <preprocessing>
                                <step>
                                    <type>JSONPATH</type>
                                    <params>$[?(@.servicename == &quot;{#SERVICENAME}&quot;)].state.first()</params>
                                </step>
                            </preprocessing>
                            <master_item>
                                <key>ipfire.services</key>
                            </master_item>
                            <trigger_prototypes>
                                <trigger_prototype>
                                    <expression>{$IPFIRE.SERVICE.TRIGGER:&quot;{#SERVICENAME}&quot;} and {last()}&lt;&gt;1</expression>
                                    <name>Service {#SERVICE} is not running</name>
                                    <priority>HIGH</priority>
                                    <description>The IPFire service {#SERVICE} is not running. If this service is manually disabled, set host macro $IPFIRE.SERVICE.TRIGGER:&quot;{#SERVICENAME}&quot; to 0 to disable this trigger.</description>
                                </trigger_prototype>
                            </trigger_prototypes>
                        </item_prototype>
                    </item_prototypes>
                    <master_item>
                        <key>ipfire.services</key>
                    </master_item>
                    <lld_macro_paths>
                        <lld_macro_path>
                            <lld_macro>{#SERVICENAME}</lld_macro>
                            <path>$.servicename</path>
                        </lld_macro_path>
                        <lld_macro_path>
                            <lld_macro>{#SERVICE}</lld_macro>
                            <path>$.service</path>
                        </lld_macro_path>
                    </lld_macro_paths>
                </discovery_rule>
            </discovery_rules>
            <macros>
                <macro>
                    <macro>{$IPFIRE.SERVICE.TRIGGER}</macro>
                    <value>1</value>
                    <description>Whether we need to trigger when a service is down. This variable can be used with context to exclude specific services.</description>
                </macro>
            </macros>
        </template>
