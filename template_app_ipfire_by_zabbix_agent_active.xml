                    <name>Firewall hits (FORWARD)</name>
                    <type>ZABBIX_ACTIVE</type>
                    <key>ipfire.net.fw.hits[&quot;POLICYFWD&quot;,&quot;DROP_FORWARD&quot;]</key>
                    <history>7d</history>
                    <units>bps</units>
                    <applications>
                        <application>
                            <name>Firewall</name>
                        </application>
                    </applications>
                    <preprocessing>
                        <step>
                            <type>CHANGE_PER_SECOND</type>
