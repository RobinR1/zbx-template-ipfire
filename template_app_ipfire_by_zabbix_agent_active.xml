                    <history>7d</history>
                    <units>bps</units>
                    <applications>
                        <application>
                            <name>Firewall</name>
                        </application>
                    </applications>
                    <preprocessing>
                        <step>
                            <type>CHANGE_PER_SECOND</type>
                            <params/>
                        </step>
                    </preprocessing>
                </item>
                <item>
                    <name>Firewall hits (OUTPUT)</name>
                    <type>ZABBIX_ACTIVE</type>
                    <key>ipfire.net.fw.hits[&quot;POLICYOUT&quot;,&quot;DROP_OUTPUT&quot;]</key>
                    <history>7d</history>
                    <units>bps</units>
                    <applications>
                        <application>
                            <name>Firewall</name>
                        </application>
                    </applications>
                    <preprocessing>
                        <step>
                            <type>CHANGE_PER_SECOND</type>
                            <params/>
                        </step>
