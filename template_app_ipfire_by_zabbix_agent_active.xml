                                    <expression>{Template App Pakfire by Zabbix Agent Active:vfs.file.time[&quot;/opt/pakfire/db/lists/server-list.db&quot;,modify].fuzzytime(176400)}=0</expression>
                                    <recovery_expression>{Template App Pakfire by Zabbix Agent Active:vfs.file.time[&quot;/opt/pakfire/db/lists/server-list.db&quot;,modify].fuzzytime(3600)}=1</recovery_expression>
                                </dependency>
                            </dependencies>
                        </trigger>
                    </triggers>
                </item>
                <item>
                    <name>Last packages-list update</name>
                    <type>ZABBIX_ACTIVE</type>
                    <key>vfs.file.time[&quot;/opt/pakfire/db/lists/packages_list.db&quot;,modify]</key>
                    <delay>10m</delay>
                    <history>7d</history>
                    <units>unixtime</units>
                    <applications>
                        <application>
                            <name>Pakfire</name>
                        </application>
                    </applications>
                    <triggers>
                        <trigger>
                            <expression>{fuzzytime(176400)}=0</expression>
                            <recovery_mode>RECOVERY_EXPRESSION</recovery_mode>
                            <recovery_expression>{fuzzytime(3600)}=1</recovery_expression>
                            <name>Pakfire: Last packages-list update &gt; 49h ago</name>
                            <priority>WARNING</priority>
                            <description>Auslösung bei Delta &gt; 49h / Recovery bei Delta &lt; 1h</description>
                            <dependencies>
                                <dependency>
                                    <name>Pakfire: Last server-list update &gt; 49h ago</name>
                                    <expression>{Template App Pakfire by Zabbix Agent Active:vfs.file.time[&quot;/opt/pakfire/db/lists/server-list.db&quot;,modify].fuzzytime(176400)}=0</expression>
                                    <recovery_expression>{Template App Pakfire by Zabbix Agent Active:vfs.file.time[&quot;/opt/pakfire/db/lists/server-list.db&quot;,modify].fuzzytime(3600)}=1</recovery_expression>
                                </dependency>
                            </dependencies>
                        </trigger>
                    </triggers>
                </item>
                <item>
                    <name>Last server-list update</name>
                    <type>ZABBIX_ACTIVE</type>
                    <key>vfs.file.time[&quot;/opt/pakfire/db/lists/server-list.db&quot;,modify]</key>
                    <delay>10m</delay>
                    <history>7d</history>
                    <units>unixtime</units>
                    <applications>
                        <application>
                            <name>Pakfire</name>
                        </application>
                    </applications>
                    <triggers>
                        <trigger>
                            <expression>{fuzzytime(176400)}=0</expression>
                            <recovery_mode>RECOVERY_EXPRESSION</recovery_mode>
                            <recovery_expression>{fuzzytime(3600)}=1</recovery_expression>
                            <name>Pakfire: Last server-list update &gt; 49h ago</name>
                            <priority>WARNING</priority>
                            <description>Auslösung bei Delta &gt; 49h / Recovery bei Delta &lt; 1h</description>
                        </trigger>
                    </triggers>
                </item>
            </items>
            <tags>
                <tag>
                    <tag>Service</tag>
                    <value>Pakfire</value>
                </tag>
            </tags>
        </template>
        <template>
            <template>Template Module IPFire General stats by Zabbix Agent Active</template>
            <name>Template Module IPFire General stats by Zabbix Agent Active</name>
            <description>Monitor some IPFire general statistics&#13;
&#13;
Created by Robin Roevens (robin.roevens (at) disroot.org)</description>
            <groups>
                <group>
                    <name>Templates/Modules</name>
                </group>
            </groups>
            <applications>
                <application>
                    <name>IPFire Status</name>
                </application>
            </applications>
            <items>
                <item>
                    <name>RDRAND hardware support</name>
                    <type>ZABBIX_ACTIVE</type>
                    <key>vfs.file.contents[&quot;/proc/cpuinfo&quot;]</key>
                    <delay>1h</delay>
                    <history>7d</history>
                    <description>Check if hardware has rdrand support.</description>
                    <applications>
                        <application>
                            <name>IPFire Status</name>
                        </application>
                    </applications>
                    <valuemap>
                        <name>Boolean Yes/No</name>
                    </valuemap>
                    <preprocessing>
                        <step>
                            <type>REGEX</type>
                            <params>rdrand
1</params>
                            <error_handler>CUSTOM_VALUE</error_handler>
                            <error_handler_params>0</error_handler_params>
                        </step>
                    </preprocessing>
                </item>
                <item>
                    <name>Entropy</name>
                    <type>ZABBIX_ACTIVE</type>
                    <key>vfs.file.contents[&quot;/proc/sys/kernel/random/entropy_avail&quot;]</key>
                    <history>7d</history>
                    <applications>
                        <application>
                            <name>IPFire Status</name>
                        </application>
                    </applications>
                </item>
                <item>
                    <name>Random Number Generator Daemon</name>
                    <type>ZABBIX_ACTIVE</type>
                    <key>vfs.file.exists[&quot;/var/run/rngd.pid&quot;]</key>
                    <history>7d</history>
                    <description>Checks if Random Number Generator Daemon (rngd) is active.</description>
                    <applications>
                        <application>
                            <name>IPFire Status</name>
                        </application>
                    </applications>
                    <valuemap>
                        <name>Service state</name>
                    </valuemap>
                </item>
            </items>
        </template>
        <template>
            <template>Template Module IPFire Network stats by Zabbix Agent Active</template>
            <name>Template Module IPFire Network stats by Zabbix Agent Active</name>
            <description>Monitors IPFire network and firewall statistics.&#13;
&#13;
Created by Robin Roevens (robin.roevens (at) disroot.org)</description>
            <groups>
                <group>
                    <name>Templates/Modules</name>
                </group>
            </groups>
            <applications>
                <application>
                    <name>Firewall</name>
                </application>
                <application>
                    <name>Network</name>
                </application>
            </applications>
            <items>
                <item>
                    <name>Firewall hits (NEWNOTSYN)</name>
                    <type>ZABBIX_ACTIVE</type>
                    <key>ipfire.net.fw.hits[&quot;NEWNOTSYN&quot;,&quot;DROP_NEWNOTSYN&quot;]</key>
                    <history>7d</history>
                    <units>bps</units>
                    <applications>
                        <application>
